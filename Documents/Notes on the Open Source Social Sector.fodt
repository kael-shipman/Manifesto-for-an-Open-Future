   <style:page-layout-properties fo:page-width="8.5in" fo:page-height="11in" style:num-format="1" style:print-orientation="portrait" fo:margin-top="1in" fo:margin-bottom="1in" fo:margin-left="1in" fo:margin-right="1in" fo:border="none" fo:padding="0in" style:writing-mode="lr-tb" style:footnote-max-height="0in">
